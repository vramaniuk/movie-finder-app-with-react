{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "react/forbid-prop-types": [
      "error",
      {
        "forbid": []
      }
    ],
    "react/prop-types": [
      0
    ],
    "arrow-body-style": [
      0,
      "always"
    ],
    "indent": [
      1
    ],
    "no-debugger": 1,
    "no-console": 1,
    "new-cap": 1,
    "strict": 1,
    "no-underscore-dangle": 1,
    "no-use-before-define": 1,
    "eol-last": 2,
    "quotes": [
      0,
      "single"
    ],
    "jsx-quotes": [
      0,
      "prefer-single"
    ],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-indent": [
      2,
      2
    ],
    "arrow-parens": [
      "error",
      "always"
    ]
  }
}
