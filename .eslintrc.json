{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
      "linebreak-style": ["error", "windows"],
      "react/forbid-prop-types": ["error", { "forbid": [] }],
      "react/prop-types": [0],
      "indent": [0]
    }
}
